{
  "name": "utils-daily",
  "version": "0.0.21",
  "description": "util functions with statistic ability",
  "types": "dist/library/utils-daily.d.ts",
  "main": "dist/library/utils-daily.common.js",
  "module": "dist/library/utils-daily.esm.js",
  "unpkg": "dist/library/utils-daily.js",
  "jsdelivr": "dist/library/utils-daily.min.js",
  "scripts": {
    "predev": "npm run test",
    "dev": "cross-env BABEL_ENV=build NODE_ENV=development node build/dev.js",
    "prebuild": "npm run lint",
    "build": "cross-env BABEL_ENV=build NODE_ENV=production node build/build.js",
    "buildAndDeploy": "npm run build && npm run deploy:server && npm run deploy:gh-pages",
    "test": "cross-env BABEL_ENV=test jest",
    "lint": "npm run test && eslint src/* test/* build/*",
    "lint:fix": "npm run test && eslint --fix src/* test/* build/*",
    "deploy:server": "node build/deploy.js",
    "deploy:gh-pages": "bash bash/gh-pages-deploy.sh",
    "release": "bash bash/release.sh"
  },
  "pre-commit": [
    "lint"
  ],
  "jest": {
    "roots": [
      "test"
    ],
    "testRegex": "test/(.*?/)?.*test.ts$",
    "testEnvironment": "jsdom",
    "moduleNameMapper": {
      "@/(.*)$": "<rootDir>/src/$1"
    }
  },
  "keywords": [
    "utils-daily"
  ],
  "author": "yakima",
  "license": "MIT",
  "private": false,
  "devDependencies": {
    "@babel/cli": "7.22.9",
    "@babel/core": "7.22.9",
    "@babel/node": "7.22.6",
    "@babel/preset-env": "7.22.9",
    "@babel/preset-typescript": "7.22.5",
    "@rollup/plugin-babel": "6.0.3",
    "@rollup/plugin-commonjs": "25.0.3",
    "@rollup/plugin-json": "6.0.0",
    "@rollup/plugin-node-resolve": "15.1.0",
    "@rollup/plugin-terser": "0.4.3",
    "@rollup/plugin-typescript": "11.1.2",
    "@types/lodash": "4.14.195",
    "@typescript-eslint/eslint-plugin": "6.1.0",
    "@typescript-eslint/parser": "6.1.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "22.4.4",
    "babel-plugin-external-helpers": "6.22.0",
    "browser-sync": "2.29.3",
    "cross-env": "5.2.1",
    "dayjs": "1.11.9",
    "eslint": "8.45.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-import-resolver-typescript": "3.5.5",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jest": "27.2.3",
    "glob": "10.3.3",
    "gulp": "4.0.2",
    "gulp-ejs": "5.1.0",
    "jest": "29.6.1",
    "jest-environment-jsdom": "29.6.2",
    "node-ssh": "13.1.0",
    "pre-commit": "1.2.2",
    "rollup": "3.26.2",
    "rollup-plugin-auto-external": "2.0.0",
    "rollup-plugin-bundle-size": "1.0.3",
    "rollup-plugin-license": "3.0.1",
    "typescript": "4.6.3",
    "vitepress": "1.0.0-beta.5"
  },
  "volta": {
    "node": "16.14.2",
    "npm": "8.5.0"
  },
  "dependencies": {
    "-": "0.0.1",
    "axios": "1.4.0",
    "D": "1.0.0",
    "fs-extra": "11.1.1",
    "lodash": "4.17.21"
  }
}
