{
  "name": "utils-daily",
  "version": "0.0.13",
  "description": "A simple yet useful frontend utility library",
  "main": "dist/libs-utils.common.js",
  "module": "dist/libs-utils.esm.js",
  "unpkg": "dist/libs-utils.js",
  "jsdelivr": "dist/utils-daily.min.js",
  "scripts": {
    "start": "npm run lint && npm run flow && npm run test && npm run docs:clean && npm run docs:jsdoc2md && npm run docs:dev",
    "build": "node build/build.js",
    "lint": "standard --verbose --fix '*.js' --parser babel-eslint --plugin flowtype",
    "flow": "flow check",
    "test": "babel-node ./spec/run.js",
    "example": "node examples/server.js",
    "docs:clean": "node build/docs-clean.js",
    "docs:jsdoc2md": "node build/docs-jsdoc2md.js",
    "docs:jsdoc2md:watch": "cross-env WATCH=1 node build/docs-jsdoc2md.js",
    "docs:dev": "cd docs && gitbook serve",
    "doc:deploy": "bash build/docs-deploy.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Yakima-Teng/utils-daily.git"
  },
  "keywords": [
    "javascript library",
    "utility function",
    "daily",
    "dom",
    "utils"
  ],
  "author": "Yakima Teng",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Yakima-Teng/utils-daily/issues"
  },
  "homepage": "https://github.com/Yakima-Teng/utils-daily#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-stage-2": "^6.24.1",
    "del": "^3.0.0",
    "eslint-plugin-flowtype": "^2.46.3",
    "express": "^4.16.3",
    "express-urlrewrite": "^1.2.0",
    "flow-bin": "^0.70.0",
    "gulp": "^3.9.1",
    "html-webpack-plugin": "^3.2.0",
    "jasmine": "^3.1.0",
    "jsdoc-babel": "^0.4.0",
    "jsdoc-to-markdown": "^4.0.1",
    "rollup": "^0.58.2",
    "rollup-plugin-babel": "^3.0.4",
    "rollup-plugin-buble": "^0.19.2",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-flow-no-whitespace": "^1.0.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-replace": "^2.0.0",
    "standard": "^11.0.1",
    "through2": "^2.0.3",
    "uglify-js": "^3.3.23",
    "webpack": "^4.6.0",
    "webpack-dev-middleware": "^3.1.2",
    "zlib": "^1.0.5"
  },
  "dependencies": {}
}
