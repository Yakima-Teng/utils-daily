{
  "name": "utils-daily",
  "version": "0.0.3",
  "description": "A simple yet useful frontend utility library",
  "main": "index.js",
  "scripts": {
    "deploy": "npm-run-all --serial docs:generateSummary docs:deploy deployToServer",
    "docs:dev": "cd docs && gitbook serve",
    "docs:generateSummary": "gulp docs:generateSummary",
    "docs:deploy": "bash ./deploy-docs.sh",
    "docs:installGitbookPlugins": "cd docs && gitbook install",
    "deployToServer": "gulp deployToServer",
    "generateIndexJS": "gulp generateIndexJS",
    "dev": "node examples/server.js",
    "build": "node build/build.js",
    "generateIndexJSAndBuild": "npm-run-all --serial generateIndexJS build",
    "flow": "flow check",
    "flow-init": "flow init",
    "test": "npm run lint && npm run flow && npm run test:unit && npm run test:e2e && npm run test:types"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Yakima-Teng/utils-daily.git"
  },
  "keywords": [
    "javascript library",
    "utility function",
    "daily",
    "dom",
    "utils"
  ],
  "author": "Yakima Teng",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Yakima-Teng/utils-daily/issues"
  },
  "homepage": "https://github.com/Yakima-Teng/utils-daily#readme",
