    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "benchmark": "^2.1.4",
    "chalk": "^2.4.1",
    "cheerio": "^1.0.0-rc.2",
    "codecov.io": "^0.1.6",
    "coveralls": "^3.0.0",
    "curl-amd": "^0.8.12",
    "del": "^3.0.0",
    "docdown": "^0.7.3",
    "dojo": "^1.13.0",
    "ecstatic": "^3.2.0",
    "es6-promise": "^4.2.4",
    "express": "^4.16.3",
    "express-urlrewrite": "^1.2.0",
    "flow-bin": "^0.70.0",
    "fs-extra": "^5.0.0",
